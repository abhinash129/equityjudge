<urlrewrite>  
<rule>
              <name>Domain Name Check for equityjudge.com</name>
                <condition name="host" operator="equal">^equityjudge.com</condition>
               <from>^(.*)$</from>
               <to type="redirect">http://www.equityjudge.com$1</to>
      </rule>
</urlrewrite>
